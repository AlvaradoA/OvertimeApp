<h1>Posts</h1>
<div class="well">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>
          #
        </th>
        <th>
          Date
        </th>
        <th>
          User
        </th>
        <th>
          Overtime Requested
        </th>
        <th>
          Rationale
        </th>
        <th>
          Status
        </th>
        <th>
          Edit
        </th>
        <th>
          Delete
        </th>
      </tr>
    </thead>
    <tbody id="posts">
      <%= render @posts %>
    </tbody>
  </table>
  <center>
    <div id="paginator">
      <%= paginate @posts, remote: true %>
    </div>
  </center>
</div>
