<h1>Audit Logs</h1>
<div class="well">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>
          #
        </th>
        <th>
          User
        </th>
        <th>
          Week Starting
        </th>
        <th>
          Confirmed at
        </th>
        <th>
          Status
        </th>
        <th>
          Edit
        </th>
      </tr>
    </thead>
    <tbody>
      <%= render @audit_logs %>
    </tbody>
  </table>

  <%= paginate @audit_logs %>
</div>
